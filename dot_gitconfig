[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[user]
	name = tkahng
	email = tkahng@gmail.com
[core]
	autocrlf = input
	editor = nvim
	excludesfile = ~/.gitignore
[diff]
  tool = nvimdiff
[alias]
  ch = checkout
  br = branch
  c = commit
  s = status
  sh = show
  sw = switch
  unstage = reset HEAD --
  lp = log --pretty=oneline --graph --decorate --all
[init]
	defaultBranch = main
